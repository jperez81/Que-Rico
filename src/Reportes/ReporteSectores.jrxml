	<queryString>
		<![CDATA[SELECT
     ordenesp.`ot` AS ot,
     ordenesp.`solicitante` AS solicitante,
     ordenesp.`fecha` AS fecha,
     ordenesp.`sector` AS sector,
     ordenesp.`detalle` AS detalle,
     ordenesp.`prioridad` AS prioridad,
     ordenesp.`estados` AS estados,
     ordenesp.`f_inicio` AS f_inicio,
     ordenesp.`sector_def` AS sector_def,
     ordenesp.`materiales` AS materiales,
     ordenesp.`obs_resp` AS obs_resp,
     ordenesp.`estado` AS estado,
     ordenesp.`f_cierre` AS f_cierre
FROM
     `ordenesp` ordenesp
WHERE 
	ordenesp.sector = $P{sectores}   AND fecha BETWEEN  $P{f_ini} and $P{f_fin}]]>
	</queryString>
