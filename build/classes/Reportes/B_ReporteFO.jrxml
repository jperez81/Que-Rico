	<queryString>
		<![CDATA[SELECT
     reporte.`id` AS id,
     reporte.`numero` AS numero,
     reporte.`fecha` AS fecha,
     reporte.`remito` AS remito,
     reporte.`chofer` AS chofer,
     reporte.`granja` AS granja,
     reporte.`cargador` AS cargador,
     reporte.`pollos` AS pollos,
     reporte.`kilos` AS kilos,
     reporte.`promedio` AS promedio,
     reporte.`muertos` AS muertos,
     reporte.`orden` AS orden
FROM
     `reporte` reporte
WHERE
     reporte.orden = $P{orden}
 AND fecha BETWEEN $P{f_ini} and $P{f_fin}]]>
	</queryString>
